@model CrudNet8MVC.Models.Contacto
@{
    ViewData["Title"] = "Detalles del contactos";
}
<h1 class="text-center">Detalle contacto Con .Net 8</h1>

<div class="row">
    <div class="offset-12">
        <div class="card-header bg-dark">
            <h3 class="text-white"><strong>Detalles del usuario</strong></h3>
        </div>
        <div class="card-body">
           
            <ul class=" list-group">
                <li class="list-group-item"><strong>Identificador: </strong>@Html.DisplayFor(m => m.Id)</li>
                <li class="list-group-item"><strong>Nombres: </strong>@Html.DisplayFor(m => m.Nombre)</li>
                <li class="list-group-item"><strong>Apellidos: </strong>@Html.DisplayFor(m => m.Apellidos)</li>
                <li class="list-group-item"><strong>Email: </strong>@Html.DisplayFor(m => m.Email)</li>
                <li class="list-group-item"><strong>Telefono: </strong>@Html.DisplayFor(m => m.Telefono)</li>
                <li class="list-group-item"><strong>Celular: </strong>@Html.DisplayFor(m => m.Celular)</li>
                <li class="list-group-item"><strong>Fecha de creacion: </strong>@Html.DisplayFor(m => m.FechaCreacion)</li>
                <li class="list-group-item"><strong>Usuario: </strong>@Html.DisplayFor(m => m.Usuario)</li>
                <li class="list-group-item"><strong>Contraseña: </strong>@Html.DisplayFor(m => m.Codigo)</li>
            </ul>

            <a asp-controller="Inicio" asp-action="Index">Volver</a>
        </div>
    </div>
</div>
